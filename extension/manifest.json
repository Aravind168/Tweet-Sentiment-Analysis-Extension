{
  "manifest_version": 3,
  "name": "Sentiment sample test",
  "version": "1.0",
  "description": "Tweet Sen",
  "content_scripts": [
    {
      "js": ["contentscript.js"],
      "matches": [
        "https://twitter.com/*"
      ]
    }
  ],
	"permissions": ["storage", "activeTab"],
  "action": {
		"default_popup": "popup.html"
	}
}